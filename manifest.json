{
  "manifest_version": 2,
  "name": "PolyDorms",
  "author": ["Justin Mo", "Josiah Pang"],
  "description": "Cal Poly Dorm Names",
  "version": "1.1",
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["popup.js"]
  },
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "clipboardRead"
   ]
}