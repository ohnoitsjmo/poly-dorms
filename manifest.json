{
  "background": {
    "scripts": ["popup.js"]
  },
  "manifest_version": 2,
  "name": "PolyDorms",
  "description": "Drag and drop Cal Poly dorm names.",
  "version": "1.0",
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "clipboardRead"
   ]
}
