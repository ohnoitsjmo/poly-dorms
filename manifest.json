{
  "manifest_version": 2,
  "name": "PolyDorms",
  "authors": ["Justin Mo", "Josiah Pang"],
  "description": "C/P CP dorm names.",
  "version": "1.0",
  "default_locale": "en",
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["popup.js"]
  },
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "clipboardRead"
   ]
}
